{
  "name": "xlsx-to-db-importer",
  "version": "1.0.0",
  "description": "A command-line tool to import Excel files into relational databases",
  "main": "index.js",
  "bin": {
    "xlsx-to-db": "./index.js"
  },
  "scripts": {
    "start": "node index.js",
    "test": "jest",
    "lint": "eslint .",
    "build": "pkg ."
  },
  "keywords": [
    "xlsx",
    "excel",
    "database",
    "import",
    "cli"
  ],
  "author": "Julio CHODATON",
  "license": "",
  "dependencies": {
    "commander": "^11.1.0",
    "dotenv": "^16.4.1",
    "knex": "^3.1.0",
    "xlsx": "^0.18.5"
  },
  "optionalDependencies": {
    "mysql2": "^3.9.1",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "pkg": "^5.8.1",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "pkg": {
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64"
    ],
    "outputPath": "dist"
  }
}